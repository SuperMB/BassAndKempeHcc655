@using HccCoffeeMaker.Models.CoffeeMakerModels;



<div style="text-align:center">
    <div style="height:60px;"></div>
    <div style="font-size:var(--largeFont)">Saved! Would you like to add another? </div>
    <div style="height:50px;"></div>

    <a href="/AddToCuration/">
        <div class="blueButton" style="width:200px;">
            <span style="position:relative;top:32px">Yes</span>
        </div>
    </a>
    <a href="/TypeSelection/">
        <div class="blueButton" style="width:200px;">
            <span style="position:relative;top:32px">No</span>
        </div>
    </a>
</div>

<div style="text-align:center">
    @foreach (var item in (ICollection<AmazonProductModel>)ViewData["products"])
    {
        @Html.Partial("~/Views/Shared/Partials/AmazonProductPartials/AmazonProductPartialSmallNoBox.cshtml", item)
    }
</div>